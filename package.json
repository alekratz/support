{
  "name": "listcommunity",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "algoliasearch": "^3.24.8",
    "base-64": "^0.1.0",
    "github-markdown-css": "^2.9.0",
    "lodash.chunk": "^4.2.0",
    "lodash.debounce": "^4.0.8",
    "mdast-util-heading-range": "^2.1.0",
    "mdast-util-toc": "^2.0.1",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "remark": "^8.0.0",
    "remark-react": "^4.0.1",
    "remark-slug": "^5.0.0",
    "tailwindcss": "^0.4.0",
    "watch": "^1.0.2"
  },
  "scripts": {
    "start": "npm-run-all -p start:css start:js",
    "start:css": "watch 'npm run build:css' ./src/css",
    "start:js": "react-scripts start",
    "build": "npm-run-all build:css build:js",
    "build:css": "tailwind build ./src/css/index.pcss -c ./tailwind-config.js -o ./src/index.css",
    "build:js": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "es5",
    "jsxBracketSameLine": true
  }
}
